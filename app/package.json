{
  "name": "@holium/realm",
  "version": "0.5.0",
  "description": "A desktop environment for Urbit",
  "private": "true",
  "scripts": {
    "build": "concurrently \"npm run build:main\" \"npm run build:renderer\" \"npm run build:cursor\"",
    "build:main": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.holium/configs/webpack.config.main.prod.ts",
    "build:renderer": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.holium/configs/webpack.config.renderer.prod.ts",
    "build:cursor": "cross-env NODE_ENV=production TS_NODE_TRANSPILE_ONLY=true webpack --config ./.holium/configs/webpack.config.cursor.prod.ts",
    "rebuild": "electron-rebuild --parallel --types prod,dev,optional --module-dir release/app",
    "lint": "cross-env NODE_ENV=development eslint . --ext .js,.jsx,.ts,.tsx",
    "package": "ts-node ./.holium/scripts/clean.js dist && npm run build && electron-builder build --publish never",
    "postinstall": "ts-node .holium/scripts/check-native-dep.js && electron-builder install-app-deps && cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.holium/configs/webpack.config.renderer.dev.dll.ts",
    "start": "ts-node ./.holium/scripts/check-port-in-use.js && npm run start:renderer",
    "start:main": "cross-env DEBUG_PROD=true INSTALL_MOON=bypass NODE_ENV=development electronmon -r ts-node/register/transpile-only ./src/main/main.ts",
    "start:preload": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.holium/configs/webpack.config.preload.dev.ts",
    "dev:cursor": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack --config ./.holium/configs/webpack.config.cursor.dev.ts",
    "start:renderer": "cross-env NODE_ENV=development TS_NODE_TRANSPILE_ONLY=true webpack serve --config ./.holium/configs/webpack.config.renderer.dev.ts",
    "test": "jest"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "cross-env NODE_ENV=development eslint --cache"
    ],
    "*.json,.{eslintrc,prettierrc}": [
      "prettier --ignore-path .eslintignore --parser json --write"
    ],
    "*.{css,scss}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ],
    "*.{html,md,yml}": [
      "prettier --ignore-path .eslintignore --single-quote --write"
    ]
  },
  "dependencies": {
    "@cliqz/adblocker-electron": "^1.23.8",
    "@dhaiwat10/react-link-preview": "^1.14.0",
    "@radix-ui/react-popover": "^1.0.0",
    "@react-hook/mouse-position": "^4.1.3",
    "@stitches/react": "^1.2.8",
    "@stripe/react-stripe-js": "^1.9.0",
    "@stripe/stripe-js": "^1.32.0",
    "@tlon/sigil-js": "^1.4.5",
    "@urbit/api": "^2.1.1",
    "async-await-queue": "^1.2.1",
    "aws-sdk": "^2.1204.0",
    "axios": "^0.26.1",
    "bcryptjs": "^2.4.3",
    "big-integer": "^1.6.51",
    "color.js": "^1.2.0",
    "cross-fetch": "^3.1.5",
    "crypto-js": "^4.1.1",
    "electron-debug": "^3.2.0",
    "electron-hot-reload": "^0.1.4",
    "electron-log": "^4.4.6",
    "electron-store": "^8.0.1",
    "electron-updater": "^4.6.5",
    "email-validator": "^2.0.4",
    "ethers": "^5.7.0",
    "eventsource": "^2.0.0",
    "extract-colors": "^1.1.22",
    "framer-motion": "^6.2.10",
    "history": "^5.3.0",
    "lodash": "^4.17.21",
    "mobx": "^6.5.0",
    "mobx-easy-form": "^1.2.2",
    "mobx-react": "^7.3.0",
    "mobx-state-tree": "^5.1.3",
    "moment": "^2.29.4",
    "mst-middlewares": "^5.1.3",
    "node-fetch": "^2.6.1",
    "normalize-url": "^7.0.3",
    "path-browserify": "^1.0.1",
    "polished": "^4.2.2",
    "protobufjs": "^7.0.0",
    "qrcode.react": "^3.1.0",
    "react": "^18.0.0",
    "react-color": "^2.19.3",
    "react-dom": "^18.0.0",
    "react-hotkeys-hook": "^3.4.7",
    "react-infinite-scroll-component": "^6.1.0",
    "react-inverted-scrollview": "^1.0.7",
    "react-router-dom": "^6.3.0",
    "react-spaces": "^0.3.2",
    "react-spinners": "^0.13.4",
    "react-virtualized-auto-sizer": "^1.0.6",
    "react-window": "^1.8.7",
    "secure-electron-store": "^3.0.3",
    "styled-components": "^5.3.3",
    "styled-system": "^5.1.5",
    "typed-emitter": "^2.1.0",
    "urbit-ob": "^5.0.1",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@pmmmwh/react-refresh-webpack-plugin": "0.5.5",
    "@teamsupercell/typings-for-css-modules-loader": "^2.5.1",
    "@testing-library/jest-dom": "^5.16.4",
    "@testing-library/react": "^13.0.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/crypto-js": "^4.1.1",
    "@types/eventsource": "^1.1.8",
    "@types/jest": "^27.4.1",
    "@types/node": "17.0.23",
    "@types/path-browserify": "^1.0.0",
    "@types/react": "^17.0.43",
    "@types/react-color": "^3.0.6",
    "@types/react-dom": "^17.0.15",
    "@types/react-test-renderer": "^17.0.1",
    "@types/react-virtualized-auto-sizer": "^1.0.1",
    "@types/react-window": "^1.8.5",
    "@types/styled-components": "^5.1.25",
    "@types/styled-system": "^5.1.15",
    "@types/terser-webpack-plugin": "^5.0.4",
    "@types/webpack-bundle-analyzer": "^4.4.1",
    "@types/webpack-env": "^1.16.3",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.20.0",
    "asar": "^3.2.0",
    "browserslist-config-erb": "^0.0.3",
    "chalk": "^4.1.2",
    "concurrently": "^7.1.0",
    "copy-webpack-plugin": "^11.0.0",
    "core-js": "^3.21.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.7.1",
    "css-minimizer-webpack-plugin": "^3.4.1",
    "detect-port": "^1.3.0",
    "electron": "^19.0.0",
    "electron-builder": "^23.3.3",
    "electron-devtools-installer": "^3.2.0",
    "electron-notarize": "^1.2.1",
    "electron-rebuild": "^3.2.7",
    "electronmon": "^2.0.2",
    "eslint": "^8.12.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-erb": "^4.0.3",
    "eslint-import-resolver-custom-alias": "^1.3.0",
    "eslint-import-resolver-typescript": "^2.7.1",
    "eslint-import-resolver-webpack": "^0.13.2",
    "eslint-plugin-compat": "^4.0.2",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jest": "^26.1.3",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-promise": "^6.0.0",
    "eslint-plugin-react": "^7.29.4",
    "eslint-plugin-react-hooks": "^4.4.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^5.5.0",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "lint-staged": "^12.3.7",
    "mini-css-extract-plugin": "^2.6.0",
    "opencollective-postinstall": "^2.0.3",
    "prettier": "^2.6.2",
    "react-refresh": "^0.12.0",
    "react-refresh-typescript": "^2.0.4",
    "react-test-renderer": "^18.0.0",
    "rimraf": "^3.0.2",
    "sass": "^1.49.11",
    "sass-loader": "^12.6.0",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "^5.3.1",
    "ts-jest": "^27.1.4",
    "ts-loader": "^9.2.8",
    "ts-node": "^10.7.0",
    "typescript": "^4.6.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.71.0",
    "webpack-bundle-analyzer": "^4.5.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.8.0",
    "webpack-merge": "^5.8.0"
  },
  "devEngines": {
    "node": ">=14.x",
    "npm": ">=7.x"
  },
  "browserslist": [],
  "prettier": {
    "overrides": [
      {
        "files": [
          ".prettierrc",
          ".eslintrc"
        ],
        "options": {
          "parser": "json"
        }
      }
    ],
    "singleQuote": true
  },
  "workspaces": {
    "packages": [
      "*"
    ],
    "nohoist": [
      "electron"
    ]
  },
  "build": {
    "electronVersion": "21.0.0-alpha.5",
    "productName": "Realm",
    "appId": "org.holium.Realm",
    "asar": true,
    "asarUnpack": "**\\*.{node,dll}",
    "files": [
      "dist",
      "node_modules",
      "package.json"
    ],
    "afterSign": ".holium/scripts/notarize.js",
    "mac": {
      "target": {
        "target": "default",
        "arch": [
          "arm64",
          "x64"
        ]
      },
      "type": "distribution",
      "hardenedRuntime": true,
      "entitlements": "assets/entitlements.mac.plist",
      "entitlementsInherit": "assets/entitlements.mac.plist",
      "gatekeeperAssess": false
    },
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "win": {
      "target": [
        "nsis"
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "Development"
    },
    "directories": {
      "app": "release/app",
      "buildResources": "assets",
      "output": "release/build"
    },
    "extraResources": [
      "./assets/**",
      "./src/main/cursor.preload.js",
      "./release/app/dist/main/cursor.js"
    ],
    "publish": {
      "provider": "github",
      "owner": "holium",
      "repo": "realm"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/holium/realm.git"
  },
  "author": {
    "name": "Holium Corporation",
    "email": "support@holium.com",
    "url": "https://github.com/holium/realm"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/holium/realm/issues"
  },
  "homepage": "https://github.com/holium/realm#readme",
  "jest": {
    "testURL": "http://localhost/",
    "testEnvironment": "jsdom",
    "transform": {
      "\\.(ts|tsx|js|jsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/.holium/mocks/fileMock.js",
      "\\.(css|less|sass|scss)$": "identity-obj-proxy"
    },
    "moduleFileExtensions": [
      "js",
      "jsx",
      "ts",
      "tsx",
      "json"
    ],
    "moduleDirectories": [
      "node_modules",
      "release/app/node_modules"
    ],
    "testPathIgnorePatterns": [
      "release/app/dist"
    ],
    "setupFiles": [
      "./.holium/scripts/check-build-exists.ts"
    ]
  }
}
